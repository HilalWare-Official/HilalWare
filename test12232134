-- Players.q692q.PlayerGui.Hilal Ware Hub.HilalWare_Frame.script area .Menü.Oyun Bilgileri.Oyun Profil.Oyun Profil alma
local function C_2e()
	local script = G2L["Oyun Profil alma_2e"];
	local imageLabel = script.Parent
	
	local gameId = game.GameId
	
	
	
	local url = "https://thumbnails.roblox.com/v1/games/icons?universeIds=" .. gameId .. "&size=150x150&format=Png"
	
	
	
	local http = game:GetService("HttpService")
	
	
	
	local function getGameThumbnail()
	
	    local success, result = pcall(function()
	
	        return http:GetAsync(url)
	
	    end)
	
	    
	
	    if success and result then
	
	        local data = http:JSONDecode(result)
	
	        if data.data and #data.data > 0 then
	
	            imageLabel.Image = data.data[1].imageUrl
	
	        end
	
	    end
	
	end
	
	
	
	getGameThumbnail()	
end;
task.spawn(C_2e);

return G2L["Hilal Ware Hub_1"], require;
