local function C_7()
	local script = G2L["LocalScript_7"];
	local button = script.Parent
	
	local screenGui = button:FindFirstAncestorOfClass("ScreenGui")
	
	
	
	button.MouseButton1Click:Connect(function()
	
	    if screenGui then
	
	        screenGui:Destroy()
	
	    end
	
	end)
	
	
end;
task.spawn(C_7);
-- Players.q692q.PlayerGui.Hilal Ware Hub.HilalWare_Frame.TAŞI
local function C_a()
	local script = G2L["TAŞI_a"];
	-- Buton (kapat)
	
	local button = script.Parent
	
	-- Taşınacak Frame (bir üst parent)
	
	local frame = button.Parent
	
	
	
	-- GUI'yi kapatma işlevi
	
	button.MouseButton1Click:Connect(function()
	
	    frame:Destroy()
	
	end)
	
	
	
	-- Sürükleme değişkenleri
	
	local dragging = false
	
	local dragInput, mousePos, framePos
	
	
	
	-- Başlarken pozisyonları al
	
	local function update(input)
	
	    local delta = input.Position - mousePos
	
	    frame.Position = UDim2.new(
	
	        framePos.X.Scale,
	
	        framePos.X.Offset + delta.X,
	
	        framePos.Y.Scale,
	
	        framePos.Y.Offset + delta.Y
	
	    )
	
	end
	
	
	
	-- Sürüklemeyi başlat
	
	frame.InputBegan:Connect(function(input)
	
	    if input.UserInputType == Enum.UserInputType.MouseButton1 then
	
	        dragging = true
	
	        mousePos = input.Position
	
	        framePos = frame.Position
	
	
	
	        input.Changed:Connect(function()
	
	            if input.UserInputState == Enum.UserInputState.End then
	
	                dragging = false
	
	            end
	
	        end)
	
	    end
	
	end)
	
	
	
	-- Sürükleme sırasında pozisyon güncelle
	
	frame.InputChanged:Connect(function(input)
	
	    if input.UserInputType == Enum.UserInputType.MouseMovement then
	
	        dragInput = input
	
	    end
	
	end)
	
	
	
	-- Render loop ile sürekli güncelle
	
	game:GetService("UserInputService").InputChanged:Connect(function(input)
	
	    if input == dragInput and dragging then
	
	        update(input)
	
	    end
	
	end)
	
	
end;
task.spawn(C_a);
-- Players.q692q.PlayerGui.Hilal Ware Hub.HilalWare_Frame.ScrollingFrame.1. buton.LocalScript
local function C_11()
	local script = G2L["LocalScript_11"];
	
	local menuButton = script.Parent
	
	
	
	
	local menuPanel = script.Parent.Parent.Parent:WaitForChild("script area"):WaitForChild("Menü")
	
	
	
	
	menuPanel.Visible = false
	
	
	
	
	menuButton.MouseButton1Click:Connect(function()	
	    menuPanel.Visible = not menuPanel.Visible
	
	end)	
end;
task.spawn(C_11);
-- Players.q692q.PlayerGui.Hilal Ware Hub.LocalScript
local function C_28()
	local script = G2L["LocalScript_28"];
	local RunService = game:GetService("RunService")
	
	
	
	
	local t = 0
	
	
	local function getAllUIStrokes(parent)
	
	    local strokes = {}
	
	    for _, desc in pairs(parent:GetDescendants()) do
	
	        if desc:IsA("UIStroke") then
	
	            table.insert(strokes, desc)
	
	        end
	
	    end
	
	    return strokes
	
	end
	
	
	
	-- GUI'nin içindeki tüm UIStroke'ları al
	
	local strokes = getAllUIStrokes(script.Parent)
	
	
	
	-- Render loop ile RGB animasyon uygula
	
	RunService.RenderStepped:Connect(function()
	
	    t += 0.01
	
	    local r = math.sin(t) * 0.5 + 0.5
	
	    local g = math.sin(t + 2) * 0.5 + 0.5
	
	    local b = math.sin(t + 4) * 0.5 + 0.5
	
	    local color = Color3.new(r, g, b)
	
	
	
	    for _, stroke in pairs(strokes) do
	
	        stroke.Color = color
	
	    end
	
	end)
	
	
end;
task.spawn(C_28);

return G2L["Hilal Ware Hub_1"], require;
