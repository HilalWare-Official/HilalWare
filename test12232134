-- Players.q692q.PlayerGui.Hilal Ware Hub.LocalScript
local function C_2()
	local script = G2L["LocalScript_2"];
	local RunService = game:GetService("RunService")
	
	
	
	
	local t = 0
	
	
	local function getAllUIStrokes(parent)
	
	    local strokes = {}
	
	    for _, desc in pairs(parent:GetDescendants()) do
	
	        if desc:IsA("UIStroke") then
	
	            table.insert(strokes, desc)
	
	        end
	
	    end
	
	    return strokes
	
	end
	
	
	
	-- GUI'nin içindeki tüm UIStroke'ları al
	
	local strokes = getAllUIStrokes(script.Parent)
	
	
	
	-- Render loop ile RGB animasyon uygula
	
	RunService.RenderStepped:Connect(function()
	
	    t += 0.01
	
	    local r = math.sin(t) * 0.5 + 0.5
	
	    local g = math.sin(t + 2) * 0.5 + 0.5
	
	    local b = math.sin(t + 4) * 0.5 + 0.5
	
	    local color = Color3.new(r, g, b)
	
	
	
	    for _, stroke in pairs(strokes) do
	
	        stroke.Color = color
	
	    end
	
	end)
	
	
end;
task.spawn(C_2);
-- Players.q692q.PlayerGui.Hilal Ware Hub.HilalWare_Frame.HilalWare.Lua.RGB
local function C_5()
	local script = G2L["RGB_5"];
	local textLabel = script.Parent
	
	
	local RunService = game:GetService("RunService")
	
	
	
	local t = 0
	
	RunService.RenderStepped:Connect(function()
	
		t = t + 0.01
	
		local r = math.sin(t) * 0.5 + 0.5
	
		local g = math.sin(t + 2) * 0.5 + 0.5
	
		local b = math.sin(t + 4) * 0.5 + 0.5
	
		textLabel.TextColor3 = Color3.new(r, g, b)
	
	end)
	
	
end;
task.spawn(C_5);
-- Players.q692q.PlayerGui.Hilal Ware Hub.HilalWare_Frame.kapat.LocalScript
local function C_8()
	local script = G2L["LocalScript_8"];
	local button = script.Parent
	
	local screenGui = button:FindFirstAncestorOfClass("ScreenGui")
	
	
	
	button.MouseButton1Click:Connect(function()
	
	    if screenGui then
	
	        screenGui:Destroy()
	
	    end
	
	end)
	
	
end;
task.spawn(C_8);
-- Players.q692q.PlayerGui.Hilal Ware Hub.HilalWare_Frame.TAŞI
local function C_b()
	local script = G2L["TAŞI_b"];
	-- Buton (kapat)
	
	local button = script.Parent
	
	-- Taşınacak Frame (bir üst parent)
	
	local frame = button.Parent
	
	
	
	-- GUI'yi kapatma işlevi
	
	button.MouseButton1Click:Connect(function()
	
	    frame:Destroy()
	
	end)
	
	
	
	-- Sürükleme değişkenleri
	
	local dragging = false
	
	local dragInput, mousePos, framePos
	
	
	
	-- Başlarken pozisyonları al
	
	local function update(input)
	
	    local delta = input.Position - mousePos
	
	    frame.Position = UDim2.new(
	
	        framePos.X.Scale,
	
	        framePos.X.Offset + delta.X,
	
	        framePos.Y.Scale,
	
	        framePos.Y.Offset + delta.Y
	
	    )
	
	end
	
	
	
	-- Sürüklemeyi başlat
	
	frame.InputBegan:Connect(function(input)
	
	    if input.UserInputType == Enum.UserInputType.MouseButton1 then
	
	        dragging = true
	
	        mousePos = input.Position
	
	        framePos = frame.Position
	
	
	
	        input.Changed:Connect(function()
	
	            if input.UserInputState == Enum.UserInputState.End then
	
	                dragging = false
	
	            end
	
	        end)
	
	    end
	
	end)
	
	
	
	-- Sürükleme sırasında pozisyon güncelle
	
	frame.InputChanged:Connect(function(input)
	
	    if input.UserInputType == Enum.UserInputType.MouseMovement then
	
	        dragInput = input
	
	    end
	
	end)
	
	
	
	-- Render loop ile sürekli güncelle
	
	game:GetService("UserInputService").InputChanged:Connect(function(input)
	
	    if input == dragInput and dragging then
	
	        update(input)
	
	    end
	
	end)
	
	
end;
task.spawn(C_b);
-- Players.q692q.PlayerGui.Hilal Ware Hub.HilalWare_Frame.Butonlar.1. buton.LocalScript
local function C_11()
	local script = G2L["LocalScript_11"];
	
	local menuButton = script.Parent
	
	
	
	
	local menuPanel = script.Parent.Parent.Parent:WaitForChild("script area"):WaitForChild("Menü")
	
	
	
	
	menuPanel.Visible = false
	
	
	
	
	menuButton.MouseButton1Click:Connect(function()	
	    menuPanel.Visible = not menuPanel.Visible
	
	end)	
end;
task.spawn(C_11);
-- Players.q692q.PlayerGui.Hilal Ware Hub.HilalWare_Frame.script area .Menü.Profil.User Name.Oyuncu İsimi Alma
local function C_1c()
	local script = G2L["Oyuncu İsimi Alma_1c"];
	
	local Players = game:GetService("Players")
	
	
	
	
	local LocalPlayer = Players.LocalPlayer
	
	
	
	-- Betiğin ebeveyni, yani metni değiştireceğimiz TextLabel elemanı.
	
	local textLabel = script.Parent
	
	
	
	
	
	if LocalPlayer then
	
	    -- TextLabel'ın Text özelliğini, oyuncunun kullanıcı adı olarak ayarla.
	
	    textLabel.Text = "User :, " .. LocalPlayer.Name	
	end	
end;
task.spawn(C_1c);
-- Players.q692q.PlayerGui.Hilal Ware Hub.HilalWare_Frame.script area .Menü.Profil.Profil.Oyuncu Profil Alma
local function C_20()
	local script = G2L["Oyuncu Profil Alma_20"];
	
	local Players = game:GetService("Players")
	
	
	
	
	local LocalPlayer = Players.LocalPlayer
	
	
	
	
	local imageLabel = script.Parent
	
	
	
	
	if LocalPlayer then	
	    local userId = LocalPlayer.UserId
	
	    	
	    local thumbnailType = Enum.ThumbnailType.HeadShot
	
	    local thumbnailSize = Enum.ThumbnailSize.Size150x150
	
	    
	
	
	    local content, isReady = Players:GetUserThumbnailAsync(userId, thumbnailType, thumbnailSize)
	
	    
	
	
	    if isReady then
	
	
	        imageLabel.Image = content
	
	    end
	
	end	
end;
task.spawn(C_20);
